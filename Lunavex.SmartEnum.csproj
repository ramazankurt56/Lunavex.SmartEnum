<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>SmartEnum</Title>
    <Version>8.0.0</Version>
    <Authors>Ramazan Kurt</Authors>
    <Company>Ramazan Kurt</Company>
    <Description>This is a smart enum library for EF Core.</Description>
    <Copyright>@2024 Ramazan Kurt. All rights reserved.</Copyright>
    <PackageIcon>lunavex1.png</PackageIcon>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/ramazankurt56/Lunavex.SmartEnum.git</RepositoryUrl>
    <PackageTags>smartenum</PackageTags>
    <PackageLicenseFile>LINCENSE</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Update="LINCENSE">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Update="lunavex1.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Update="README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
